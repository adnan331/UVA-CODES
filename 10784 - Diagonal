// taking any two points from the diagonal to make a line ,so number of lines: nC2 - n
// here given the value of x
// nC2 - n = x
// n(n-1)/2 - n =x
// n = 3 + sqrt(9+8x)/2


#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;
int main()
{
    long long n,d,no=0;
    while(cin>>d && d!=0)
    {
        no++;
        n=ceil((3+sqrt(9+8*d))/2);
        printf("Case %lld: %lld\n",no,n);
    }

    return 0;

}
