#include<iostream>
#include<cstdio>
#include<string>
using namespace std;
int main()
{
    int n,l;
    string s;
    cin>>n;
    while(n--)
    {
        cin>>s;
        l = s.size();
        if(l==3)
        {
            if(s[0]=='o'&&s[1]=='n' || s[0]=='o'&&s[2]=='e' || s[1]=='n'&&s[2]=='e')
                cout<<"1\n";
            else if(s[0]=='t'&&s[1]=='w' || s[0]=='t'&&s[2]=='o' || s[1]=='w'&&s[2]=='o')
                cout<<"2\n";
            else if(s[0]=='s'&&s[1]=='i' || s[0]=='s'&&s[2]=='x' || s[1]=='i'&&s[2]=='x')
                cout<<"6\n";
                else if(s[0]=='t'&&s[1]=='e' || s[0]=='t'&&s[2]=='n' || s[1]=='e'&&s[2]=='n')
                cout<<"10\n";
        }
        else if(l==4)
        {
            if(s[0]=='f'&&s[1]=='o'&&s[2]=='u' || s[0]=='f'&&s[1]=='o'&&s[3]=='r' || s[0]=='f'&&s[2]=='u'&&s[3]=='r' || s[1]=='o'&&s[2]=='u'&&s[3]=='r')
                cout<<"4\n";
                        else if(s[0]=='f'&&s[1]=='i'&&s[2]=='v' || s[0]=='f'&&s[1]=='i'&&s[3]=='e' || s[0]=='f'&&s[2]=='v'&&s[3]=='e' || s[1]=='i'&&s[2]=='v'&&s[3]=='e')
                            cout<<"5\n";
                          else if(s[0]=='n'&&s[1]=='i'&&s[2]=='n' || s[0]=='n'&&s[1]=='i'&&s[3]=='e'|| s[0]=='n'&&s[2]=='n'&&s[3]=='e' || s[1]=='i'&&s[2]=='n'&&s[3]=='e')
                                cout<<"9\n";
        }
        else
        {
             if(s[0]=='t'&&s[1]=='h'&&s[2]=='r'&&s[3]=='e'||s[0]=='t'&&s[1]=='h'&&s[2]=='r'&&s[4]=='e'||s[0]=='t'&&s[1]=='h'&&s[3]=='e'&&s[4]=='e'||s[0]=='t'&&s[2]=='r'&&s[3]=='e'&&s[4]=='e'||s[1]=='h'&&s[2]=='r'&&s[3]=='e'&&s[4]=='e')
                cout<<"3\n";
            else if(s[0]=='s'&&s[1]=='e'&&s[2]=='v'&&s[3]=='e'||s[0]=='s'&&s[1]=='e'&&s[2]=='v'&&s[4]=='n'||s[0]=='s'&&s[1]=='e'&&s[3]=='e'&&s[4]=='n'||s[0]=='s'&&s[2]=='v'&&s[3]=='e'&&s[4]=='n'||s[1]=='e'&&s[2]=='v'&&s[3]=='e'&&s[4]=='n')
                cout<<"7\n";
                else if(s[0]=='e'&&s[1]=='i'&&s[2]=='g'&&s[3]=='h'||s[0]=='e'&&s[1]=='i'&&s[2]=='g'&&s[4]=='t'||s[0]=='e'&&s[1]=='i'&&s[3]=='h'&&s[4]=='t'||s[0]=='e'&&s[2]=='g'&&s[3]=='h'&&s[4]=='t'||s[1]=='i'&&s[2]=='g'&&s[3]=='h'&&s[4]=='t')
                cout<<"8\n";
        }

    }
    return 0;
}
