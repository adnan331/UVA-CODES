#include<iostream>
#include<cstdio>
using namespace std;
long long int factorial(long long int num)
{
    if(num==1) return 1;
    else
        return (num*factorial(num-1));
}
int main()
{
    long long  n;
    long long int ans;
    while(scanf("%lld",&n)==1)
    {
        if(n>13)
            cout<<"Overflow!\n";
        else if(n>=0 && n<=7)
            cout<<"Underflow!\n";
         else if(n<0 && n%2==0)
                cout<<"Underflow!\n";
         else if(n<0 && n%2!=0)
            cout<<"Overflow!\n";
        else
        {
            ans = factorial(n);
            cout<<ans<<endl;
        }
    }
    return 0;
}
